<Cadastros>
    MDBoxLayout:
        size_hint_x: .8
        adaptive_height: True
        spacing: dp(10)
        pos_hint: {'center_x': .5, 'center_y': .5}
        orientation: 'vertical'
        MDTextField:
            id: inp_nome
            hint_text: 'Nome'
            helper_text: 'Fulano de Tal'
            helper_text_mode: 'on_focus'
        MDTextField:
            id: inp_nascimento
            hint_text: 'Nascimento'
            helper_text: 'DD/MM/AAAA'
            helper_text_mode: 'on_focus'
        MDTextField:
            id: inp_cidade
            hint_text: 'Cidade'
            helper_text: 'São José dos Campos'
            helper_text_mode: 'on_focus'
        MDTextField:
            id: inp_cpf
            hint_text: 'Cpf'
            helper_text: '11122233300'
            helper_text_mode: 'on_focus'
        MDTextField:
            id: inp_salario
            hint_text: 'Salario'
            helper_text: '1000'
            helper_text_mode: 'on_focus'
        MDRectangleFlatButton:
            text: 'Salvar'
            on_release:
                app.cadastrar(inp_nome.text, inp_nascimento.text, inp_cidade.text, inp_cpf.text, inp_salario.text)
